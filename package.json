  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "commitizen": "^4.2.2",
    "eslint": "^7.11.0",
    "husky": "^4.3.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.3"
